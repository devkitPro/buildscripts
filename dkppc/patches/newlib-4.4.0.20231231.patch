+}
+
+char *getcwd(char *buf, size_t size) {
+
+	struct _reent *r = _REENT;
+
+	if (size == 0) {
+		if (buf != NULL) {
+			r->_errno = EINVAL;
+			return NULL;
+		}
+		buf = malloc(PATH_MAX);
+		size = PATH_MAX;
+	}
+
+	if (buf == NULL) {
+		r->_errno = EINVAL;
+		return NULL;
+	}
+
+	if ( size < (strnlen (_current_working_directory, PATH_MAX) + 1)) {
+		r->_errno = ERANGE;
+		return NULL;
+	}
+
+	strncpy (buf, _current_working_directory, size);
+
+	return buf;
