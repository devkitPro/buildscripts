+	if(!(NAME)) __libc_lock_init(&(NAME),1);	\
+	__libc_lock_try_acquire(&(NAME))
+
+#define __lock_release(NAME) \
+	__libc_lock_release(&(NAME))
+
+#define __lock_release_recursive(NAME) \
+	__libc_lock_release(&(NAME))
+
