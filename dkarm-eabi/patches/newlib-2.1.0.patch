--- newlib-2.1.0/libgloss/libsysbase/chmod.c	1970-01-01 01:00:00.000000000 +0100
+++ newlib-2.1.0-arm/libgloss/libsysbase/chmod.c	2014-04-01 14:35:06.000000000 +0100
@@ -0,0 +1,25 @@
+#include "config.h"
+#include <_ansi.h>
+#include <_syslist.h>
+#include <sys/types.h>
+#include <sys/stat.h>
+#include <sys/iosupport.h>
+#include <errno.h>
+
+int chmod(const char *path, mode_t mode) {
+	int	ret,dev;
+	struct _reent *r = _REENT;
+
+	/* Get device from path name */
+	dev = FindDevice(path);
+	
+	if ((dev < 0) || (devoptab_list[dev]->chmod_r == NULL)) {
+		r->_errno = ENODEV;
+		ret = -1;
+	} else {
+		ret = devoptab_list[dev]->chmod_r(r,path,mode);
+	}
+
+	return ret;
+}
+
