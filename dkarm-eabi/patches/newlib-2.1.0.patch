diff -Nbaur newlib-2.1.0/libgloss/libsysbase/handle_manager.c newlib-2.1.0-arm/libgloss/libsysbase/handle_manager.c
--- newlib-2.1.0/libgloss/libsysbase/handle_manager.c	1970-01-01 01:00:00.000000000 +0100
+++ newlib-2.1.0-arm/libgloss/libsysbase/handle_manager.c	2014-04-01 14:35:06.000000000 +0100
@@ -0,0 +1,47 @@
+#include <stdlib.h>
+#include <stdio.h>
+
+#include "handle_manager.h"
+
+#define MAX_HANDLES 1024
+
+static __handle* handles[MAX_HANDLES];
+
+
+void __release_handle(int fd) {
+
+	if ( fd < 3 || fd >= MAX_HANDLES + 3 ) return;
+
+	fd -= 3;
+	__handle* handle = handles[fd];
+	if ( NULL != handle ) {
+		free(handle);
+		handles[fd] = NULL;
+	}
+
+}
+
+int  __alloc_handle(int size) {
+
+	int i;
+
+	for ( i = 0; i < MAX_HANDLES; i++ ) {
+		if ( handles[i] == NULL ) break;
+	}
+	
+	if ( i == MAX_HANDLES ) return -1;
+	
+	handles[i] = malloc(size);
+	
+	if ( NULL == handles[i] ) return -1;
+	
+	return i+3; 
+}
+
+__handle *__get_handle(int fd) {
+
+	if ( fd < 3 || fd > MAX_HANDLES + 3 ) return NULL;
+	
+	return handles[fd-3];
+
+}
